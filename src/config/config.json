{
    "data_set / source_system": "priyamovielens",
    "type": "file_ingestion",
    "schedule": "cron(0 0 * * ? *)",
    "source_bucket": "priya-soure-bucket",
    "source_folder": "priyamovielens",
    "target_bucket": "priya-raw-bucket",
    "pipeline": [
        {
            "data_asset": "movie_rating",
            "raw": {
                "source_bucket": "priya-soure-bucket",
                "source_folder": "priyamovielens",
                "target_bucket": "priya-raw-bucket",
                "partition": " MONTH ",
                "file_pattern": "rating*",
                "file_type": "csv"
            },
            "staging": {},
            "publish": {}
        },
        {
            "data_asset": "genome_scores_data",
            "raw": {
                "source_bucket": "priya-soure-bucket",
                "source_folder": "priyamovielens",
                "target_bucket": "priya-raw-bucket",
                "partition": "DAY",
                "file_pattern": "genome_scores*",
                "file_type": "csv"
            },
            "staging": {},
            "publish": {}
        },
        {
            "data_asset": "genome_tags",
            "raw": {
                "source_bucket": "priya-soure-bucket",
                "source_folder": "priyamovielens",
                "target_bucket": "priya-raw-bucket",
