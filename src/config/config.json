                "file_pattern": "genome_tag*",
                "file_type": "csv"

            },
            "staging": {},
            "publish": {}
        },
        {
            "data_asset": "movie_links",
            "raw": {
                "source_bucket": "priya-soure-bucket",
                "source_folder": "priyamovielens",
                "target_bucket": "priya-raw-bucket",
                "partition": "DAY",
                "file_pattern": "link*",
                "file_type": "csv"
            },
            "staging": {},
            "publish": {}
        },
        {
            "data_asset": "movie_tags",
            "raw": {
                "source_bucket": "priya-soure-bucket",
                "source_folder": "priyamovielens",
                "target_bucket": "priya-raw-bucket",
                "partition": " MONTH ",
                "file_pattern": "tags",
                "file_type": "csv"
            },
            "staging": {},
            "publish": {}
        },
        {
            "data_asset": "movies",
            "raw": {
                "source_bucket": "priya-soure-bucket",
                "source_folder": "priyamovielens",
                "target_bucket": "priya-raw-bucket",
                "partition": " MONTH ",
                "file_pattern": "movie",
